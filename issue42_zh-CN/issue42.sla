   <ITEXT CAB="17" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="在Python&#x1d;2.x中执行如下代码&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="WenQuanYi Micro Hei Regular" CSHX="5" CSHY="-5" CSTROKE="Black" />
