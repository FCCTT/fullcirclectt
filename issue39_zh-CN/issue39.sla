  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="10" gHeight="3.00933e-252" gWidth="0" LANGUAGE="English" NUMPO="16" PLINEART="2" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="13.2" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="813.544" ImageRes="2" GROUPS="3511041 " LOCKR="0" IFONT="DejaVu Sans Book" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="11.3386" PCOLOR2="None" NEXTITEM="-1" NUMGROUP="1" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="0" HEIGHT="483.528" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="11" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="-1" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 813.544 0 813.544 0 813.544 0 813.544 0 813.544 483.528 813.544 483.528 813.544 483.528 813.544 483.528 0 483.528 0 483.528 0 483.528 0 483.528 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="4" OwnPage="12" LAYER="11" BOOKMARK="0" gYpos="2.75965e-255" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="0" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="7726.8" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="114.174" NUMCO="16" POCOOR="0 0 0 0 813.544 0 813.544 0 813.544 0 813.544 0 813.544 483.528 813.544 483.528 813.544 483.528 813.544 483.528 0 483.528 0 483.528 0 483.528 0 483.528 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="我们可以使用类似vmstat或者top的优秀工具来监视我们的系统。如果文本界面不适合，你还可以使用KSysGuard或者GNOME&#x1d;System&#x1d;Monitor。" CSHADE2="100" CKERN="100" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="不过对某些人而言，工具并不能提供足够详细的信息。比如说，因为我们要讨论的是虚拟内存，就会出现这样的问题：在已使用的512MB内存中，有多少" CSHADE2="100" CKERN="110" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="12" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="是分配给了匿名分页？vmstat和top并不能让你很明显、清楚地了解这些。&#x5;" CSHADE2="100" CKERN="100" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="那么我们应该怎么办呢？我们要从源头出发：top、vmstat之类工具都是从/proc中的对应文件来获取信息的。通过直接查看其中的信息，你可以直接看到由内核提供的信息。但是其中有些信息的格式不是很明显，要知道其中的第一列或者第二列什么意思还需要知道一些知识，但是作为回报你可以知道尽可能详细的信息。&#x5;" CSHADE2="100" CKERN="60" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="系统虚拟内存数据在/proc/meminfo中，还有/proc/loadavg是系统负载信息，/proc/cpuinfo是处理器特性和性能的数据，而/proc/vmstat中则是虚拟内存数据的详细信息。&#x5;" CSHADE2="100" CKERN="50" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="通过不使用如vmstat之类工具查看/proc文件的明显好处是你仅仅需要一个简单的文本查看器，例如cat。要是你想的话，你还可以使用右面这样的命令格式化输出：&#x5;" CSHADE2="100" CKERN="80" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;&#x5;&#x5;&#x5;&#x5;&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="上面的输出来自我的笔记本（装有2GB内存）。  &#x5;" CSHADE2="100" CKERN="50" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="先从最简单的开始：&#x1d;MemTotal。它表示可以被内核分配的物理内存大小。这里的“分配”是什么意思呢？简单来说就是可以在内核地址空间被寻址的大小，包括永久和临时分配的。 &#x5;" CSHADE2="100" CKERN="50" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="那是什么意思呢？虽然BIOS(Basic&#x1d;Input/Output&#x1d;System)可以识别比如说2GB，但有可能内核并不能寻址那么多。这要取决于内核在编译时是如何配置的。基本上有三种选择： &#x5;" CSHADE2="100" CKERN="50" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
