   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="#!/usr/bin/env python&#x5;# client2.py&#x5;  &#x5;from socket import *&#x5;from time import time&#x5;from time import sleep&#x5;import sys&#x5;BUFSIZE = 4096&#x5;  &#x5;class CmdLine:&#x5;    def __init__(self,host):&#x5;        self.HOST = host&#x5;        self.PORT = 29876&#x5;        self.ADDR = (self.HOST,self.PORT)&#x5;        self.sock = None&#x5;  &#x5;    def makeConnection(self):&#x5;        self.sock = socket( AF_INET,SOCK_STREAM)&#x5;        self.sock.connect(self.ADDR)&#x5;  &#x5;    def sendCmd(self, cmd):&#x5;        self.sock.send(cmd)&#x5;  &#x5;    def getResults(self):&#x5;        data = self.sock.recv(BUFSIZE)&#x5;        print data&#x5;&#x5;if __name__ == '__main__':&#x5;    conn = CmdLine('localhost')&#x5;    conn.makeConnection()&#x5;    conn.sendCmd('ls -al')&#x5;    conn.getResults()&#x5;    conn.sendCmd('BYE')" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
