  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="10" gHeight="7.05943e-258" gWidth="7.05917e-258" LANGUAGE="English" NUMPO="16" PLINEART="1" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="13.2" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="609.718" ImageRes="2" GROUPS="" LOCKR="0" IFONT="DejaVu Sans Book" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="11.3386" PCOLOR2="None" NEXTITEM="4" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="0" HEIGHT="490.331" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="11" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="381" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="Textlk485" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 609.718 0 609.718 0 609.718 0 609.718 0 609.718 490.331 609.718 490.331 609.718 490.331 609.718 490.331 0 490.331 0 490.331 0 490.331 0 490.331 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="3" OwnPage="30" LAYER="37" BOOKMARK="0" gYpos="7.05892e-258" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="7.05867e-258" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="19154.9" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="318" NUMCO="16" POCOOR="0 0 0 0 609.718 0 609.718 0 609.718 0 609.718 0 609.718 490.331 609.718 490.331 609.718 490.331 609.718 490.331 0 490.331 0 490.331 0 490.331 0 490.331 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="9" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="但就算跳过动画你也不会错过什么重要剧情。相比游戏剧情来说，《毁灭战士&#x1d;3》更注重于游戏内容和环境。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="article inner header" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="《毁灭战士&#x1d;3》和其他的第一人称射击游戏类似：游戏的目的是依次到达各个目标地点，沿途射杀一切怪物，完成一个个关卡。《毁灭战士&#x1d;3》照例使用流行的奔跑和枪械模式。你可以使用一系列标准的武器，例如手枪、散弹枪和突击步枪，它们的性能都很好，开火时的音效十分震撼。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="article inner header" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="这款游戏极具恐怖特点。它并不是通过鲜血来制造恐怖感，而是利用各种效果和物品让你吓得跳起来。光线效果是达到恐怖感的一大要素——闪烁的微光随时可能熄灭，昏暗的走廊中任何一个拐角都有可能潜伏着敌人。为了增强游戏的恐怖感，它使用了其他游戏都没有用过的一个有趣的机制。通常的游戏中，你可以在端着武器的同时使用你的手电筒。但是在《毁灭战士&#x1d;3》中你只能使用其中的一个。在灯光下端着枪让你感觉安心，但一旦灯光熄灭了，你就需要放下武器，打开手电了。在你放下武器的时候，敌人随时可能冲上来。当灯光熄灭后的几秒钟真的让人精神紧张——你换上手电查看四周，突然发现了敌人，于是立刻换上枪向暗处射击。游戏音效非常好，共同营造了恐怖气氛。正在坍塌的科学站的声音和正在接近的敌人的咆哮都让游戏氛围更加恐怖。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="article inner header" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="游戏画面非常惊人，可谓是Linux上最好的游戏了。阴影和光照效果在所有的游戏中都可谓数一数二。你需要一块性能强大的显卡来支持这款游戏华丽的特效，不过这款游戏也可以支持低端配置。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="article inner header" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="《毁灭战士&#x1d;3》也有在线和局域网多人模式。然而，这非常令人失望。只有少量的游戏模式和地图，毫无令人眼前一亮的创意。在线社区的玩家数量也能反映出多人模式有多糟" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
