   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH=" 0           1           2&#x5;0+-----------+-----------+&#x5; | Button 1  | Button 2  |&#x5;1+-----------+-----------+&#x5; |       Button 3        |&#x5;2+-----------+-----------+&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
