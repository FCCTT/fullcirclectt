            <ITEXT CH="上个月33期发布之后，UbuntumemberChrisJohnston给我写电邮指出Byobu("/>
            <ITEXT FONT="AR PL UKai CN Book" FONTSIZE="14" FEATURES="inherit underline" FCOLOR="HTML Link" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="https://edge.launchpad.net/byobu"/>
