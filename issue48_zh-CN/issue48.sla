   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH=".&#x5;#EXTM3U &#x5;Adult Contemporary/Chris Rea/Collection/02 - On The Beach.mp3 &#x5;Adult Contemporary/Chris Rea/Collection/07 - Fool (If You Think It's Over).mp3 &#x5;Adult Contemporary/Chris Rea/Collection/11 - Looking For The Summer.mp3&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
