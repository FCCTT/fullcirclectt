  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="0" gHeight="312.999" gWidth="6.89947e-314" LANGUAGE="English" NUMPO="16" PLINEART="1" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="13.2" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="396.434" ImageRes="2" GROUPS="" LOCKR="0" IFONT="DejaVu Sans Book" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="11.3386" PCOLOR2="None" NEXTITEM="579" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="0" HEIGHT="531.996" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="11" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="2" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 396.434 0 396.434 0 396.434 0 396.434 0 396.434 531.996 396.434 531.996 396.434 531.996 396.434 531.996 0 531.996 0 531.996 0 531.996 0 531.996 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="2" OwnPage="16" LAYER="29" BOOKMARK="0" gYpos="-3.381e+10" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="2.8461e-154" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="10212.8" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="531.284" NUMCO="16" POCOOR="0 0 0 0 396.434 0 396.434 0 396.434 0 396.434 0 396.434 531.996 396.434 531.996 396.434 531.996 396.434 531.996 0 531.996 0 531.996 0 531.996 0 531.996 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="对我们的表达式而言，需要匹配'&lt;'和'>'之间的内容作为HTML标记，这是很容易的，表达式需要一个&lt;，紧跟着一些字符.+?，并以>结尾，+?与+*一样，但是一种非贪婪匹配，意味着仅匹配最短合法字符，包括&lt;>之间的内容，而不包括其他内容，这才是我们想要的。&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH=">>>summary&#x1d;=&#x1d;data.entries[0].summary&#x5;>>>import re&#x5;>>>pattern = '&lt;.+?>'&#x5;>>>temp&#x1d;=&#x1d;re.sub(pattern,,summary)&#x5;>>>temp&#x5;u'\nCurrent Conditions:\nHaze, 13 C\nForecast:\nThu - Rain. High: 14 Low: 9\nFri - Light Rain. High: 12 Low: 7\n\n Full Forecast at Yahoo! Weather\n(provided by The Weather Channel)'&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="现在已经没有HTML标记了，但是还有换行符。我们可以使用标准的string模块将其分割为一个列表，但是我们需要重新加载re模块。我们需要搜索换行符，这可以通过在字符串前面添加一个r将其变为raw string。&#x5;&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH=">>>&#x1d;temp&#x1d;=&#x1d;re.split(r'\n',temp)&#x5;>>>&#x1d;temp&#x5;[u, u'Current Conditions:', u'Haze, 13 C', u'Forecast:', u'Thu - Rain. High: 14 Low: 9', u'Fri - Light Rain. High: 12 Low: 7', u, u'Full Forecast at Yahoo! Weather', u'(provided by The Weather Channel)'] >>>temp[2] u'Haze, 13 C'&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="可以看到我们假设结果的第三个元素是我们想要的字符串，为了从这里提取温度，还需要进一步利用正则表达式提取数字。&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH=">>>temp = re.findall(u'[0-9]+',temp[2])[0]&#x5;>>>temp&#x5;u'13'&#x5;>>>&#x5;>>> temp = int(temp)&#x5;>>> temp&#x5;13&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="最后一步是使用Python的内置方法将字符串转换为数字，在现实世界中我们可能将这些步骤结合起来提高效率，但是其开销相当小，将其分解为多个步骤可以使其看起来更加明了。现在要做的就是实现桌面背景与天气之间的联系。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UMing CN Light" CSHX="5" CSHY="-5" CSTROKE="Black" />
