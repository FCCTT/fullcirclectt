YPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" ANACTION="0 595 0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
