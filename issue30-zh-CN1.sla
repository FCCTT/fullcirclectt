  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="10" gHeight="9.50215e-252" gWidth="-3.33103e-124" LANGUAGE="English" NUMPO="16" PLINEART="1" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="13.2" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="605.718" ImageRes="2" GROUPS="" LOCKR="0" IFONT="DejaVu Sans Book" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="11.3386" PCOLOR2="None" NEXTITEM="467" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="0" HEIGHT="488.609" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="11" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="465" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 605.718 0 605.718 0 605.718 0 605.718 0 605.718 488.609 605.718 488.609 605.718 488.609 605.718 488.609 0 488.609 0 488.609 0 488.609 0 488.609 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="3" OwnPage="6" LAYER="41" BOOKMARK="0" gYpos="-3.01434e+170" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="-3.33103e-124" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="3910" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="322" NUMCO="16" POCOOR="0 0 0 0 605.718 0 605.718 0 605.718 0 605.718 0 605.718 488.609 605.718 488.609 605.718 488.609 605.718 488.609 0 488.609 0 488.609 0 488.609 0 488.609 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="型；类就是实现建模的一种方法。打比方说，我们家里有三只狗，一只米格鲁猎兔犬(Beagle)，一只拉布拉多犬(Lab)，还有一只德国牧羊犬与蓝色赫勒犬的混血狗(Shepherd/Heeler)。它们都是狗，却各有不同。它们有共同属性，也有各自的独特属性。例如，米格鲁猎兔犬矮胖棕黑，脾气暴躁；拉布拉多犬则是中等身材，黑色皮毛，性格随和；德国牧羊犬与蓝赫勒犬的混血狗则又高又瘦，也是黑色的，而且更狂躁。马上，这些不同的属性就显现出来了。矮、中等、高都是高度属性；脾气暴躁、性格随和、狂躁便是性格属性。从动物行为这点来看，我们还可以考虑吃、睡、玩等动作。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Adobe Kaiti Std R" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="这三个都属于“Dog”类。对于属性的描述，我们可以用Dog.Name、Dog.Height、Dog.Build(瘦骨嶙峋、圆胖等)和Dog.Color；我们也可以使用动物行为，如Dog.Bark、Dog.Eat、Dog.Sleep等。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Adobe Kaiti Std R" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="正如我之前指出的，每条狗都属于不同的品种。每一个种类都是“Dog”的子类。我们用图表说明如下：&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Adobe Kaiti Std R" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH=" &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="        /--Beagle&#x5;Dog ---|-- Lab&#x5;        \--Shepherd/Heeler&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="每一个子类继承了&quot;Dog&quot;类的所有属性。因此，如果我们创建一个Beagle的实例，它将拥有其父类(Dog类)的所有属性。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Adobe Kaiti Std R" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="Beagle = Dog()&#x5;Beagle.Name = 'Archie'&#x5;Beagle.Height = 'Short'&#x5;Beagle.Build = 'Chubby'&#x5;Beagle.Color = 'Brown'&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="18" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="不知各位读者理解与否？现在，让我们创建一个笼统的“Dog”类。创建类时，首先输入关键字“class”以及类的名称。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Adobe Kaiti Std R" CSHX="5" CSHY="-5" CSTROKE="Black" />
