   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="easily add it to the above parameter. For example, if you want to add the skype repository, which is at http://download.skype.com/linux/repos/debian, you can append:&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="skype download.skype.com/linux/repos/debian;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="to the end of the above path_map parameter to do that.&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;Now edit /etc/default/apt-cacher and set AUTOSTART to 1 to enable apt-cacher to boot at system startup. Then restart apt-cacher by running:&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="sudo /etc/init.d/apt-cacher restart&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;Test your apt-cacher installation by accessing:&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="HTML Link" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="http://localhost:3142" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;&#x5;in your browser. If apt-cacher is running you will see the report when you access this URL.&#x5;&#x5;You can now use a Ubuntu CD/Ubuntu DVD and copy all the packages from the CD/DVD into the apt-cacher cache. Replace '/media/UbuntuCD' with whatever is the correct path where the CD or the DVD is mounted.&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="sudo /usr/share/apt-cacher/apt-cacher-import.pl /media/UbuntuCD&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;Once apt-cacher is configured on the local package repository machine, it is time to update the sources.list on all the client machines in the intranet to use the local cache. On each PC create a copy of the original sources.list:&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="sudo cp /etc/apt/sources.list /etc/apt/sources.list.bak&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="and edit the file to add the local repository:&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="sudo gedit /etc/apt/sources.list&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="12" CULP="-0.1" CH="Copy and paste the following code into sources.list, replacing [YourLocalRepositoryMachineIP]with the IP address of the local package repository that you have configured:&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="DejaVu Sans Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="## Ubuntu Packages&#x5;deb http://[YourLocalRepositoryMachineIP]:3142/ubuntu/ gutsy main restricted multiverse universe&#x5;deb-src http://[YourLocalRepositoryMachineIP]:3142/ubuntu/ gutsy main restricted multiverse universe&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
