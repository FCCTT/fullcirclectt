            <ITEXT FONT="Adobe Heiti Std R" CH="这是跑在我的Compaq"/>
            <nbspace/>
            <ITEXT FONT="Adobe Heiti Std R" CH="Presario"/>
            <nbspace/>
            <ITEXT FONT="Adobe Heiti Std R" CH="2500笔记本上的漂亮Ubuntu"/>
