            <para PARENT="Command Line Commands"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT CH="耐心等待一两分钟，你会得到下面所示的输出信息。"/>
            <para PARENT="Basic Text + indent"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT CH="根据上述建议重新启动虚拟机系统。"/>
            <para PARENT="Basic Text + indent"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT CH="当你再度回来后，你需要修改 X11/xorg.conf文件。在你对它进行任何修改之前，最好先对它进行备份。可以使用nano、gedit、mousepad或者你喜欢的任何文本编辑器对它进行编辑。"/>
            <para PARENT="Basic Text + indent"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT CH="打开一个终端窗口，然后输入下面的命令（我使用的是mousepad）："/>
            <para PARENT="Basic Text + indent"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="11" FEATURES="inherit" FCOLOR="header bar" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="sudo mousepad /etc/X11/xorg.conf"/>
            <para PARENT="Command Line Commands"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT CH="向下滚动到："/>
            <para PARENT="Basic Text + indent"/>
