            <para PARENT="Article Inner Headers"/>
            <para PARENT="Article Inner Headers"/>
            <ITEXT CH="没错，这个可以让你对现场直播电视节目进"/>
            <ITEXT FONT="Adobe Song Std L" CH="行暂停和回放"/>
            <ITEXT FONT="Adobe Song Std L" FONTSIZE="12" FCOLOR="Black" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" CH="的神奇的设备"/>
