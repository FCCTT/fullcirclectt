            <ITEXT FONT="Adobe Heiti Std R" FCOLOR="Code" FSHADE="100" CH="bios"/>
            <nbspace/>
            <ITEXT FONT="Adobe Heiti Std R" FCOLOR="Code" FSHADE="100" CH="year acpi="/>
