            <ITEXT KERN="5" CH="拷贝网站是否仅作私人用途？"/>
            <para PARENT="Basic Text"/>
            <ITEXT FONT="DejaVu Sans Book" FONTSIZE="12" FEATURES="inherit" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="5" CH="•"/>
            <nbspace/>
            <ITEXT KERN="5" CH="没有得到授权的情况下，请勿自行建立镜像站点。"/>
            <para PARENT="Basic Text"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT FONT="Adobe Heiti Std R" KERN="5" CH="此外，请勿把该软件用于侵蚀网络带宽，或其他不良企图"/>
            <ITEXT KERN="5" CH="："/>
            <para PARENT="Basic Text"/>
            <para PARENT="Basic Text + indent"/>
            <ITEXT FONT="DejaVu Sans Book" FONTSIZE="12" FEATURES="inherit" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="5" CH="•"/>
            <nbspace/>
            <ITEXT KERN="5" CH="请勿对网站进行无限制的下载（属于道德问题，上面提过的）；因为镜像整个网站会使服务器端网络带宽超载，特别是当你抓取太多动态生成的网页时候。反过来,这样做也会糟蹋你自己的带宽和存储空间！因此敬请遵守！"/>
            <para PARENT="Basic Text"/>
            <ITEXT FONT="DejaVu Sans Book" FONTSIZE="12" FEATURES="inherit" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="5" CH="•"/>
