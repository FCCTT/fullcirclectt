            <ITEXT FONT="Adobe Heiti Std R" FCOLOR="Code" FSHADE="100" CH="DV5T笔记本电脑上安装了Ubuntu9.04操作系统，每次开机的时候内部的无线网卡和外置的D–Link无线网卡会一同启动。通过在终端输入命令我可以手动停止内部网卡。有什么好办法可以在开机的时候就能够停止内部无线网卡？"/>
            <para PARENT="Basic Text - 1st Paragraph"/>
            <para PARENT="Basic Text - 1st Paragraph"/>
            <ITEXT FONT="Adobe Heiti Std R" FONTSIZE="12" FEATURES="inherit" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="A"/>
            <ITEXT FONT="Adobe Song Std L" CH="你可以打开/etc/network/interfaces文件，并且将里面的“auto"/>
            <nbspace/>
            <ITEXT FONT="Adobe Song Std L" CH="eth1”这行"/>
            <ITEXT FONT="Adobe Song Std L" FONTSIZE="11" FCOLOR="Black" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" CH="注释掉"/>
            <ITEXT FONT="Adobe Song Std L" CH="，假设eth1是内置无线网卡的名字，例如："/>
            <para PARENT="Basic Text - 1st Paragraph"/>
            <para PARENT="Basic Text - 1st Paragraph"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="11" FEATURES="inherit" FCOLOR="header bar" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="auto eth0"/>
            <para PARENT="Command Line Commands"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="11" FEATURES="inherit" FCOLOR="header bar" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="iface eth0 inet dhcp"/>
            <para PARENT="Command Line Commands"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="11" FEATURES="inherit" FCOLOR="header bar" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="#auto wlan1"/>
            <para PARENT="Command Line Commands"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="11" FEATURES="inherit" FCOLOR="header bar" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="iface eth1 inet dhcp"/>
