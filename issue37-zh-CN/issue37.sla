   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="&#x1d;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
