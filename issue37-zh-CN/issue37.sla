  <PAGEOBJECT OnMasterPage="" BottomLine="0" REXTRA="0" ALIGN="10" gHeight="3.00933e-252" gWidth="0" LANGUAGE="English" NUMPO="16" PLINEART="2" TXTSCALE="100" RightLine="0" LOCALSCX="1" LINESP="13.2" ROT="0" TXTSHX="5" TXTSTROKE="Black" WIDTH="606.829" ImageRes="2" GROUPS="" LOCKR="0" IFONT="DejaVu Sans Book" LOCALSCY="1" NAMEDLST="" TXTSHY="-5" isInline="0" AUTOTEXT="0" FLIPPEDV="0" PCOLOR="None" RADRECT="0" REVERS="0" PRINTABLE="1" RATIO="1" FLIPPEDH="0" COLGAP="11.3386" PCOLOR2="None" NEXTITEM="-1" NUMGROUP="0" TransValue="0" PLINEEND="0" TXTSTW="-0.1" FRTYPE="0" PTYPE="4" ImageClip="" isTableItem="0" TEXTFLOW2="0" SHADE2="100" TXTBASE="0" PWIDTH="0" HEIGHT="483.528" DASHOFF="0" PFILE2="" PFILE="" TEXTFLOW3="0" ISIZE="11" PLTSHOW="0" LINESPMode="0" TXTSTYLE="0" CLIPEDIT="0" BACKITEM="419" TransValueS="0" EMBEDDED="1" PFILE3="" ANNAME="" SHADE="100" TXTULP="-0.1" fillRule="1" COCOOR="0 0 0 0 606.829 0 606.829 0 606.829 0 606.829 0 606.829 483.528 606.829 483.528 606.829 483.528 606.829 483.528 0 483.528 0 483.528 0 483.528 0 483.528 0 0 0 0 " BASEOF="0" PICART="1" TXTKERN="0" COLUMNS="3" OwnPage="13" LAYER="11" BOOKMARK="0" gYpos="2.75965e-255" startArrowIndex="0" TopLine="0" LOCK="0" EPROF="" gXpos="0" DASHS="" IRENDER="1" TEXTFLOW="0" TXTFILLSH="100" YPOS="8362.08" ANNOTATION="0" LOCALX="0" GRTYP="0" XPOS="320.889" NUMCO="16" POCOOR="0 0 0 0 606.829 0 606.829 0 606.829 0 606.829 0 606.829 483.528 606.829 483.528 606.829 483.528 606.829 483.528 0 483.528 0 483.528 0 483.528 0 483.528 0 0 0 0 " EXTRA="0" LOCALY="0" NUMDASH="0" LeftLine="0" PRFILE="" TEXTRA="0" SCALETYPE="1" TXTOUT="1" TXTFILL="Black" endArrowIndex="0" BEXTRA="0" PLINEJOIN="0" TXTSTP="-0.1" TXTULW="-0.1" TXTSCALEV="100" TXTSTRSH="100" >
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="就出现了：Xbox&#x1d;360不能识别网络上的电脑，因为Ubuntu没有包含可识别的媒体服务器。一个可能的解决方案叫做uShare，它是专为家庭影院电脑设计的Linux发行版GeeXboX的一部分&#x1d;——&#x1d;文章最后有关于GeeXboX的链接。uShare实际上是创建了给Xbox&#x1d;360（和其他设备）在家庭网络中提供媒体文件信息的UPnP（Universal&#x18;Plug&#x18;and&#x18;Play）服务器。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="Ubuntu源里面有这个程序&#x1d;——&#x1d;将被安装的软件包叫做ushare。在安装成功之后，你需要打开位于/etc/ushare.conf的配置文件并编辑它以使它符合你的需要。这里提供一个配置它的例子（只有重要的选项）：&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="USHARE_NAME=Name_Of_Your_Server&#x5;&#x5;USHARE_IFACE=wlan0;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="(列出将被使用的网络设备；例如：wlan0,&#x1d;eth0,&#x1d;...)&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="11" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;USHARE_PORT=49153;&#x5;&#x5;USHARE_DIR=/path/to/media&#x5;&#x5;USHARE_ENABLE_XBOX=yes;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="尽管如UPnP的字面意思是即插即用，但是你还是需要在保存配置文件并通过在终端输入命令：ushare&#x1d;&#x18;x&#x1d;（要激活Xbox兼容性“&#x18;x”是必须的）开启uShare&#x18;server之后重启你的Xbox。我的电脑是通过无线适配器连接到网络的，uShare提示我“Interface&#x1d;wlan0&#x1d;is&#x1d;down.”&#x1d;&#x18;&#x1d;忽略它就好。即使出现这个错误，服务器还是会正常工作。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="在服务器运行的时候，导航到你Xbox的视频库，选择你创建的媒体服务器。 &#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="10" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="如果媒体服务器没有出现在列表上，你必须检查防火墙和路由器（例如：49153&#x1d;转发端口）。我的路由器直到我重新配置NAT的时候才没有阻止我的连接。如果一切都没问题的话，你将可以在你的长沙发上拿着Xbox&#x1d;360的遥控器享受最喜欢的电影了。&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="0" CCOLOR="header bar" CBASE="0" CSTW="-0.1" CSIZE="11" CULP="-0.1" CH="&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="Black" CBASE="0" CSTW="-0.1" CSIZE="14" CULP="-0.1" CH="相关链接：&#x5;[1]&#x1d;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="AR PL UKai CN Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
   <ITEXT CAB="5" CCOLOR="HTML Link" CBASE="0" CSTW="-0.1" CSIZE="13" CULP="-0.1" CH="http://ushare.geexbox.org/" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="8" CFONT="Garuda Book" CSHX="5" CSHY="-5" CSTROKE="Black" />
